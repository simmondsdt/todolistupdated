<%= form_for [@list, @item] do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
    <br />
  <%= f.label :description %>
  <%= f.text_area :description %>
    <br />
  <%= f.label :urgency %>
  <%= f.text_field :urgency %>
    <br />
  <%= f.label :complete %>
  <%= f.check_box :complete %>
    <br />
  <%= f.submit %>
<% end %>
