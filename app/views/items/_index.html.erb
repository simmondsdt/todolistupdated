<% @list.items.each do |item| %>
<ul>
  <li>
      <%= item.name %>
      <%= item.description %>
      <%= item.urgency %>
      <%= item.complete %>
  </li>
</ul>
<%= link_to 'Show', list_item_path(item.list_id, item.id) %> --
<%= link_to 'Edit', edit_list_item_path(item.list_id, item.id) %> --
<%= link_to 'Delete', list_item_path(item.list_id, item.id), method: :delete, data: {confirm: "Are you sure you would like to delete this item?"} %>
<% end %>
